---
import { GitHubIcon, MastodonIcon, SteamIcon, XIcon } from "vue3-simple-icons"
---

<div class="flex justify-start gap-col-2">
  <a
    href="https://github.com/NanamiNakano"
    target="_blank"
    class="rounded-full bg-gray-8 text-white p-2 flex items-center justify-center w-12 h-12"
  >
    <GitHubIcon />
  </a>
  <a
    href="https://mas.to/@thynanami"
    target="_blank"
    class="rounded-full bg-violet-8 text-white p-2 flex items-center justify-center w-12 h-12"
  >
    <MastodonIcon />
  </a>
  <a
    href="https://x.com/thynanami"
    target="_blank"
    class="rounded-full bg-black text-white p-2 flex items-center justify-center w-12 h-12"
  >
    <XIcon />
  </a>
  <a
    href="https://steamcommunity.com/id/thynanami"
    target="_blank"
    class="rounded-full bg-gray-8 text-white p-2 flex items-center justify-center w-12 h-12"
  >
    <SteamIcon />
  </a>
</div>
